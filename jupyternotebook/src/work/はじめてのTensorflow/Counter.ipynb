   "source": [
    "# Counterクラスの読み込み\n",
    "from collections import Counter\n",
    "\n",
    "list = ['a','b','c','a','a','c']\n",
    "\n",
    "counter = Counter(list)\n",
    "print(counter)\n",
    "\n",
    "for elem, cnt in counter.most_common():\n",
    "    print(elem,cnt)"
